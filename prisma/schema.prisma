generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"  // This will create a database file in your prisma directory
}

model Visitor {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  ipAddress String?
  userAgent String?
  path      String?
}